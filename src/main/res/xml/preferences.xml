<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedResources">
    <EditTextPreference
        android:key="gasp_reviews_uri"
        android:title="Gasp! Reviews Settings"
        android:dialogTitle="Gasp! Reviews URL"
        android:defaultValue="@string/gasp_reviews_url" />
    <EditTextPreference
        android:key="gasp_restaurants_uri"
        android:title="Gasp! Restaurants Settings"
        android:dialogTitle="Gasp! Restaurants URL"
        android:defaultValue="@string/gasp_restaurants_url" />
    <EditTextPreference
        android:key="gasp_users_uri"
        android:title="Gasp! Users Settings"
        android:dialogTitle="Gasp! Users URL"
        android:defaultValue="@string/gasp_users_url" />
    <EditTextPreference
        android:key="gasp_push_uri"
        android:title="Gasp! GCM Push Notification Settings"
        android:dialogTitle="Gasp! GCM Push Server URL"
        android:defaultValue="@string/gasp_push_url" />
</PreferenceScreen>